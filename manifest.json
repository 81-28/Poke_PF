{
    "name": "Poke_PF",
    "version": "1.0",
    "description": "Pokemon Get",
    "manifest_version": 2,
    "permissions": ["tabs"],
    "icons": {
      "16": "images/pokeball.png",
      "48": "images/greatball.png",
      "128": "images/ultraball.png"
    },

    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "images/pokeball.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  